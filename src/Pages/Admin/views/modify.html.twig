{% block body %}
	<title>
		{% block title %}Espace Administrateur - Modification{% endblock %}
	</title>
	
	<div class="flex align">
		<button class="add">
			<a class="clean-text" href="/admin/add">Ajouter</a>
		</button>
		<button class="modify">
			<a class="clean-text" href="/admin/modify/list">Modifier</a>
		</button>
		<button class="delete">
			<a class="clean-text" href="/admin/modify/list">Supprimer</a>
		</button>
	</div>

	<div class="title-test align">Sur quelle page voulez-vous modifier des données ?</div>

	<div class="modify-list">
		<div class="flex align">
			<button class="modify">
				<a class="clean-text" href="/admin/modify/accueil">Accueil</a>
			</button>
			<button class="modify">
				<a class="clean-text" href="/admin/modify/association">Association</a>
			</button>
		</div>
		<div class="flex align">
			<button class="modify">
				<a class="clean-text" href="/admin/modify/enfance">Enfance</a>
			</button>
			<button class="modify">
				<a class="clean-text" href="/admin/modify/jeunesse">Jeunesse</a>
			</button>
		</div>
		<div class="flex align">
			<button class="modify">
				<a class="clean-text" href="/admin/modify/famille">Famille/Vie Sociale</a>
			</button>
			<button class="modify">
				<a class="clean-text" href="/admin/modify/accompagnement">Accompagnement</a>
			</button>
		</div>
		<div class="flex align">
			<button class="modify">
				<a class="clean-text" href="/admin/modify/partenaires">Partenaires</a>
			</button>
			<button class="modify">
				<a class="clean-text" href="/admin/modify/telechargements">Téléchargements</a>
			</button>
		</div>
	</div>

	{% block importmap %}
		{{ importmap('app') }}
		{% block stylesheets %}{% endblock %}
	{% endblock %}
{% endblock %}
